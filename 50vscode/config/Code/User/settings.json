{
    "markdown.extension.orderedList.marker": "ordered",
    "cSpell.checkLimit": 10240,
    "cSpell.minWordLength": 3,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "window.zoomLevel": 0,
    "cSpell.userWords": [
        "COE",
        "DJS",
        "EOD",
        "ETO",
        "FUQ",
        "Gitfarm",
        "HOTW",
        "LDM",
        "MCM",
        "SDE",
        "horizonte",
        "mitigations",
        "oncilla",
        "parallelizable",
        "performant",
        "prioritizations",
        "recurse",
    ],
    "bracketPairColorizer.forceIterationColorCycle": true,
    "markdownlint.config": {
        "header-style": { "style": "atx"},
        "ul-indent": { "indent": 4 },
        "no-trailing-punctuation": { "punctuation": ".,;:!" },
        "fenced-code-language": false,
        "single-h1": false,
        "no-bare-urls": false,
        "ol-prefix": {
            "style": "ordered"
        },
        "no-inilne-html": false
    },
    "editor.rules": [80],
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "workbench.colorCustomizations": {
        "activityBar.background": "#000C1D",
        "activityBar.border": "#102a44",
        "sideBar.background": "#001122",
        "sideBar.border": "#102a44",
        "sideBar.foreground": "#8BADC1",
    },
    "workbench.colorTheme": "Night Owl",
    "editor.tokenColorCustomizations": {
        "[Night Owl No Italics]": {
            "textMateRules": [
                {
                  "name": "Markdown Heading Underline (H1)",
                  "scope": "markup.heading.setext.1.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
                {
                  "name": "Markdown Heading Underline (H2)",
                  "scope": "markup.heading.setext.2.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
            ]
        },
        "[Night Owl]": {
            "textMateRules": [
                {
                  "name": "Markdown Heading Underline (H1)",
                  "scope": "markup.heading.setext.1.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
                {
                  "name": "Markdown Heading Underline (H2)",
                  "scope": "markup.heading.setext.2.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
            ]
        }
    },
    "breadcrumbs.enabled": true,
    "update.channel": "none",
    "clang-format.fallbackStyle": "google",
    "python.linting.pylintPath": "/usr/bin/gpylint",
    "files.associations": {
        "BUILD": "bazel",
        "METADATA": "bazel",
        "WORKSPACE": "bazel",
        "*.gss": "css",
    },
    "[bazel]": {
        "editor.formatOnSave": true,
    },
    "workbench.iconTheme": "vscode-icons",
    "window.titleBarStyle": "custom",
}

