{
    "markdown.extension.orderedList.marker": "ordered",
    "cSpell.checkLimit": 10240,
    "cSpell.minWordLength": 3,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "window.zoomLevel": 0,
    "cSpell.userWords": [
        "COE",
        "DJS",
        "EOD",
        "ETO",
        "FUQ",
        "Gitfarm",
        "HOTW",
        "LDM",
        "MCM",
        "SDE",
        "harrigal",
        "heilmanc",
        "horizonte",
        "mickolas",
        "mitigations",
        "oncilla",
        "parallelizable",
        "performant",
        "prioritizations",
        "rcmill",
        "recurse",
        "ritesh",
        "riteshah",
        "sepidj"
    ],
    "bracketPairColorizer.forceIterationColorCycle": true,
    "markdownlint.config": {
        "header-style": { "style": "setext_with_atx"},
        "ul-indent": { "indent": 4 },
        "no-trailing-punctuation": { "punctuation": ".,;:!" },
        "fenced-code-language": false,
        "single-h1": false,
        "no-bare-urls": false,
        "ol-prefix": {
            "style": "ordered"
        }
    },
    "editor.detectIndentation": false,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "workbench.colorCustomizations": {
        "activityBar.background": "#000C1D",
        "activityBar.border": "#102a44",
        "sideBar.background": "#001122",
        "sideBar.border": "#102a44",
        "sideBar.foreground": "#8BADC1",
    },
    "workbench.colorTheme": "Night Owl No Italics",
    "editor.tokenColorCustomizations": {
        "[Night Owl No Italics]": {
            "textMateRules": [
                {
                  "name": "Markdown Heading Underline (H1)",
                  "scope": "markup.heading.setext.1.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
                {
                  "name": "Markdown Heading Underline (H2)",
                  "scope": "markup.heading.setext.2.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
            ]
        },
        "[Night Owl]": {
            "textMateRules": [
                {
                  "name": "Markdown Heading Underline (H1)",
                  "scope": "markup.heading.setext.1.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
                {
                  "name": "Markdown Heading Underline (H2)",
                  "scope": "markup.heading.setext.2.markdown",
                  "settings": {
                    "foreground": "#82b1ff"
                  }
                },
            ]
        }
    },
}
