#!/usr/bin/env zsh
#
# Dock the laptop (transfers i3 workspaces 2-8 to DP1-1)
#

i3-msg "workspace 1; move workspace to output DP-6"
i3-msg "workspace 2; move workspace to output DP-6"
i3-msg "workspace 3; move workspace to output DP-6"
i3-msg "workspace 4; move workspace to output DP-6"
i3-msg "workspace 5; move workspace to output DP-6"
i3-msg "workspace 6; move workspace to output DP-6"
i3-msg "workspace 7; move workspace to output DP-6"
i3-msg "workspace 8; move workspace to output DP-7"
i3-msg "workspace 9; move workspace to output DP-7"

sleep 0.1s

i3-msg workspace 1
i3-msg workspace 2

sleep 0.5s

# i3-msg restart

